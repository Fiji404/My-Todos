{
    "name": "my-todos",
    "version": "1.0.0",
    "scripts": {
        "dev": "concurrently \"npm:astro-dev\" \"npm:tailwind-dev\"",
        "build": "concurrently \"npm:astro-build\" \"npm:tailwind-build\"",
        "astro-dev": "astro dev",
        "astro-build": "astro build",
        "tailwind-dev": "tailwind -i ./src/styles/index.css -o ./src/styles/app.css --watch",
        "tailwind-build": "tailwind build -i ./src/styles/index.css -o ./src/styles/app.css"
    },
    "license": "ISC",
    "dependencies": {
        "@astrojs/tailwind": "^2.1.3",
        "@popperjs/core": "^2.11.6",
        "astro": "^1.6.14",
        "js-datepicker": "^5.18.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/uuid": "^8.3.4",
        "concurrently": "^7.6.0",
        "tailwindcss": "^3.2.4",
        "typescript": "^4.8.4"
    }
}
