{
    "name": "my-todos",
    "version": "1.0.0",
    "scripts": {
        "dev": "concurrently \"npm:vite\" \"npm:tailwind-dev\"",
        "build": "concurrently \"npm:vite\" \"npm:tailwind-build\"",
        "vite": "vite",
        "tailwind-dev": "tailwind -i ./src/styles/index.css -o ./src/styles/app.css --watch",
        "tailwind-build": "tailwind build ./src/styles/index.css -o ./src/styles/app.css"
    },
    "license": "ISC",
    "dependencies": {
        "@popperjs/core": "^2.11.6",
        "js-datepicker": "^5.18.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/uuid": "^8.3.4",
        "concurrently": "^7.6.0",
        "tailwindcss": "^3.2.4",
        "typescript": "^4.8.4",
        "vite": "^3.2.4"
    }
}
